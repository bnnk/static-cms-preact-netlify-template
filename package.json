{
  "name": "static-cms-preact-netlify-template",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A starter for a blog powered by Preact and Static CMS",
  "repository": "https://github.com/StaticJsCMS/static-cms-preact-netlify-template",
  "bugs": "https://github.com/StaticJsCMS/static-cms-preact-netlify-template/issues",
  "keywords": [
    "preact",
    "static",
    "cms",
    "netlify"
  ],
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --prerenderUrls ./prerender-urls.js",
    "dev": "preact watch --prerender --prerenderUrls ./prerender-urls.js",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "@preact/prerender-data-provider": "3.0.1",
    "imagemin-mozjpeg": "10.0.0",
    "imagemin-webpack-plugin": "2.4.2",
    "markdown-to-jsx": "7.1.9",
    "preact": "10.13.1",
    "preact-cli-plugin-netlify": "1.6.0",
    "preact-render-to-string": "5.2.6",
    "preact-router": "4.1.0",
    "parse-md": "3.0.3"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-synacor": "3.0.5",
    "esm": "3.2.25",
    "glob": "9.2.1",
    "markdown": "0.5.0",
    "per-env": "1.0.2",
    "preact-cli": "3.4.5",
    "preact-render-spy": "1.3.0",
    "react-syntax-highlighter": "15.5.0",
    "webpack-bundle-analyzer": "4.8.0"
  },
  "jest": {
    "preset": "jest-preset-preact"
  }
}
